@using Microsoft.AspNet.Identity
@if (Request.IsAuthenticated)
{
    using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", @class = "navbar-left" }))
    {
        @Html.AntiForgeryToken()

        <ul>
            <li>
                Hello, @User.Identity.GetUserName()
                @*@Html.ActionLink("Hello " + User.Identity.GetUserName() + "!", "Index", "Manage", routeValues: null, htmlAttributes: new { title = "Manage" })*@
            </li>
            <li style="padding-left:140px;">
                <a href="javascript:document.getElementById('logoutForm').submit()" class="use1"><span>Log off</span></a>
            </li>
            
        </ul>
    }
}
else
